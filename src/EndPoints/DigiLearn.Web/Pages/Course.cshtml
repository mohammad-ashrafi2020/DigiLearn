@page "{slug}"
@model DigiLearn.Web.Pages.CourseModel
@{
    ViewData["title"] = Model.Course.Title;
}
@section meta
    {
    <link rel="stylesheet" href="/css/single-Page.min.css">
    <link rel="stylesheet" href="/videoJs/video-js.css">
}
<div class="main-content">
    <div class="breadcrumb">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12  col-xl-8 col-lg-7">
                    <h1>@Model.Course.Title</h1>
                    <ul>
                        <li>
                            <a href="/">
                                خانه
                                <i class="fa fa-chevron-left"></i>
                            </a>
                        </li>
                        <li>
                            <a href="/courses?category=@Model.Course.MainCategory.Slug">
                                @Model.Course.MainCategory.Title
                                <i class="fa fa-chevron-left"></i>
                            </a>
                        </li>
                        <li>
                            <a href="/courses?category=@Model.Course.SubCategory.Slug">
                                @Model.Course.SubCategory.Title
                                <i class="fa fa-chevron-left"></i>
                            </a>
                        </li>
                        <li>
                            <a class="current">
                                @Model.Course.Title
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="col-12 col-md-6 col-sm-8 col-xl-4 col-lg-5 left-side">
                    <div class="course-shortlink shadow-sm">
                        <span class="title"> لینک کوتاه </span>
                        <span class="link">
                            codeyad.com/Q/xG99
                        </span>
                        <a href="javascript:void(0)" data-toggle="tooltip" title="" class="copy-btn" data-clipboard-text="codeyad.com/c/xG99" data-original-title="کپی"><i class="zmdi zmdi-copy"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid mb-2 mt-4">
        <div class="row">
            <div class="col-12 col-xl-3 col-lg-4  mb-3">
                <div class="course-info p-3">

                    <div class="price mt-2 mb-3">
                        <div class="d-inline-block">
                            <i class="zmdi zmdi-money text-success"></i>
                            قیمت دوره :
                        </div>
                        <div class="pull-left d-inline-block ">
                            @if (Model.Course.Price == 0)
                            {
                                <label class="text-success">رایگان</label>
                            }
                            else
                            {
                                <span style="font-size: 15px;">@Model.Course.Price.ToString("#,0")</span> <label class="text-success">تومان</label>
                            }
                        </div>
                    </div>
                    <div class="teacher mb-3">
                        <div class="d-inline-block">
                            <i class="zmdi zmdi-account-o"></i>
                            مدرس :
                        </div>
                        <div class="pull-left d-inline-block">
                            <a href="/master/@Model.Course.Teacher.UserName">@Model.Course.Teacher.User.Name @Model.Course.Teacher.User.Family</a>
                        </div>
                    </div>
                    <div class="students mb-3">
                        <div class="d-inline-block">
                            <i class="zmdi zmdi-accounts-outline"></i>
                            تعداد دانشجویان
                        </div>
                        <div class="pull-left d-inline-block">
                            <span>150</span> <label>نفر</label>
                        </div>
                    </div>
                    <div class="countEpisode mb-3">
                        <div class="d-inline-block">
                            <i class="fa fa-th-list text-uppercase"></i>
                            تعداد قسمت :
                        </div>
                        <div class="pull-left d-inline-block">
                            <span>
                                @Model.Course.Sections.Sum(r=>r.Episodes.Count) قسمت
                            </span>
                        </div>
                    </div>
                    <div class="time mb-3">
                        <div class="d-inline-block">
                            <i class="zmdi zmdi-time"></i>
                            مدت زمان آموزش:
                        </div>
                        <div class="pull-left d-inline-block">
                            <span>@Model.Course.GetDuration()</span>
                        </div>
                    </div>

                    <div class="status mb-3">
                        <div class="d-inline-block">
                            <i class="fa fa-info"></i>
                            وضعیت دوره :
                        </div>
                        <div class="pull-left d-inline-block">
                            <span class="text-primary">
                                @Model.Course.GetCourseStatus()
                            </span>
                        </div>

                    </div>
                    <div class="level mb-3">
                        <div class="d-inline-block">
                            <i class="zmdi zmdi-layers"></i>
                            سطح دوره :
                        </div>
                        <div class="pull-left d-inline-block">
                            <span>
                                @Model.Course.GetCourseLevel()
                            </span>
                        </div>
                    </div>
                    <a href="#" class="btn btn-block btn-success">ثبت نام</a>

                </div>
                <div class="teacher shadow mt-3">
                    <div class="title">
                        <h5>مدرس دوره</h5>
                    </div>
                    <div class="middel  p-2 bg-white">
                        <img class="d-inline-block" src="images/avatar04.png" alt="">

                        <a href="#">
                            محمد اشرافی
                        </a>

                    </div>
                    <div class="about-teacher  bg-white">
                        <p>
                            اول داستان، طراح گرافیک بودم و ۲ سالی به عنوان طراح مشغول بودم، بعد به برنامه‌نویسی
                            علاقمند شدم و الان بیشتر از ۱۰ ساله که عاشق کدزنی و چالش‌های پروژه‌های مالش‌های
                            پروژه‌های مالش‌های پروژه‌های مالش‌های پروژه‌های مختلفم. به تدریس
                            علاقه خاصی دارم و دوست دارم دانشی که در این راه بدست آوردم را در اختیار دیگران هم قرار
                            بدم.
                        </p>
                        <div class="filter"></div>
                    </div>
                </div>
                <a href="/questions.html" class="btn btn-block btn-primary mt-2">پرسش و پاسخ</a>
                <a href="#" class="btn btn-block btn-danger mt-2">
                    <i class="fa fa-heart"></i>
                    افزودن به علاقه مندی ها
                </a>

            </div>
            <div class="col-12 col-xl-9 col-lg-8 ">
                <div class="course-body">
                    <div id="instructions">
